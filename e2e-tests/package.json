{
	"name": "e2e-tests",
	"version": "1.0.0",
	"description": "Cypress tests for web and Cucumber for API",
	"scripts": {
		"lint": "eslint ./",
		"lint:fix": "eslint ./ --fix",
		"test:open": "cypress open",
		"test:e2e": "cypress run -e TAGS='not (@wip or @ignore)'",
		"test:e2e:demo": "cypress run --headed -b chrome -e TAGS='not (@wip or @ignore)',demoDelay=1000",
		"test:e2e:postprocess": "node ./reporter.js",
		"test:e2e:smoke": "cypress run -e TAGS='not (@wip or @ignore) and @smoketest' --config video=false,baseUrl=$BASEURL",
		"test:e2e:api": "npx cucumber-js --config cucumber/cucumber.json"
	},
	"author": "",
	"license": "ISC",
	"cypress-cucumber-preprocessor": {
		"nonGlobalStepDefinitions": true,
		"cucumberJson": {
			"generate": true,
			"outputFolder": "cypress/cucumber-json",
			"filePrefix": "",
			"fileSuffix": ".cucumber"
		}
	},
	"devDependencies": {
		"@cucumber/cucumber": "^8.9.0",
		"axe-core": "^4.1.3",
		"axios": "^1.2.1",
		"chai": "^4.3.7",
		"cypress": "^7.4.0",
		"cypress-axe": "^0.14.0",
		"cypress-cucumber-preprocessor": "^4.0.3",
		"eslint": "^8.17.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-cypress": "^2.12.1",
		"multiple-cucumber-html-reporter": "^1.18.0"
	}
}
