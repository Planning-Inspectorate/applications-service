<nav class="moj-primary-navigation pins-modded-full-width pins-modded-locale-selector" aria-label="{{ t('global.primaryNavigation.primaryNavigationAria') }}">
  <ul class="moj-primary-navigation__list">
    {% for link in global.primaryNavigationLinks %}
      <li class="moj-primary-navigation__item">
        <a class="moj-primary-navigation__link" {% if link.active %} aria-current="page" {% endif %} href="{{ link.href }}">{{ link.text }}</a>
      </li>
    {% endfor %}
  </ul>

  {% if featureFlag.allowWelshTranslation %}
    {% include "./locale-selector.njk" %}
  {% endif %}
</nav>
