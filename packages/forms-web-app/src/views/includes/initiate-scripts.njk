<script
	nonce={{ cspNonce }}
>
	var initiateScriptsConfig = {{ initiateScriptsConfig | dump | safe }}</script>

{% if initiateScriptsConfig | length %}
	<script
		nonce={{ cspNonce }}
		defer="true"
		id="triggerMe"
		src="/public/scripts/initiate.script.js"
		type="text/javascript"
	>

	</script>

	<script nonce={{ cspNonce }}>
		document.querySelector('#triggerMe').addEventListener('load', function () {
			var initiateSite = new appScripts.initiate();
			initiateSite.scripts(initiateScriptsConfig, "{{ cspNonce }}");
		})
	</script>
{% endif %}
