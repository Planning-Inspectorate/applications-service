<section>
	<h2 class="govuk-heading-m">
		{{ t('projectsIndex.aboutProject.heading1') }}
	</h2>

	<p class="govuk-body">
		{{ t('projectsIndex.aboutProject.paragraph1') }}
		{{ proposal }}
	</p>

	<p class="govuk-body">
		{{ t('projectsIndex.aboutProject.paragraph2') }}
		{{ applicationData.promoterName }}
	</p>

	<p class="govuk-body">
		{{ applicationData.summary | safe }}
	</p>

	{% if applicationData.webAddress %}
		<a class="govuk-link" href="{{ applicationData.webAddress }}">
			{{ t('projectsIndex.aboutProject.linkText1') }}
		</a>
	{% endif %}
</section>