<section>
	<h2 class="govuk-heading-m">About the project</h2>

	<p class="govuk-body">Type of application: {{ proposal }}</p>

	<p class="govuk-body">Name of applicant: {{ applicationData.promoterName }}</p>

	<p class="govuk-body">{{ applicationData.summary|safe }}</p>

	{% if applicationData.webAddress %}
		<a class="govuk-link" href="{{ applicationData.webAddress }}">View the developer's website</a>
	{% endif %}
</section>
