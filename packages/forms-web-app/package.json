{
	"name": "@pins/forms-web-app",
	"version": "0.0.0",
	"private": true,
	"engines": {
		"node": ">=20.0.0 <21.0.0"
	},
	"scripts": {
		"build": "npm run build:client && npm run build:sass && npm run build:app",
		"build:app": "cp -Rf ./src ./dist",
		"build:client": "webpack --config webpack.config.js",
		"build:sass": "sass src/sass/main.scss:src/public/stylesheets/main.css --style=compressed --load-path=../../ --no-source-map",
		"lint": "eslint ./",
		"lint:fix": "eslint ./ --fix",
		"prebuild": "rm -Rf ./dist",
		"start": "node dist/server.js",
		"start:dev": "npm run start:dev:server && npm run start:dev:client && npm run build:sass && npm run watch:sass",
		"start:dev:client": "webpack --config webpack.config.js --watch",
		"start:dev:debug": "npm run start:dev:server:debug && npm run start:dev:client && npm run build:sass && npm run watch:sass",
		"start:dev:server": "nodemon src/server.js | pino-pretty -tlc",
		"start:dev:server:debug": "nodemon --inspect=0.0.0.0 src/server.js",
		"test": "jest",
		"test:cov": "jest --coverage --verbose",
		"test:watch": "jest --watch --runInBand",
		"test:update": "jest -u",
		"watch:sass": "nodemon -e scss -x \"npm run build:sass\""
	},
	"dependencies": {
		"@ministryofjustice/frontend": "*",
		"@pins/common": "*",
		"@planning-inspectorate/pins-components": "*",
		"@testing-library/dom": "*",
		"@testing-library/jest-dom": "*",
		"@testing-library/user-event": "*",
		"applicationinsights": "*",
		"chokidar": "*",
		"compression": "*",
		"connect-redis": "*",
		"cookie-parser": "*",
		"dayjs": "*",
		"dotenv": "*",
		"express": "*",
		"express-async-errors": "*",
		"express-fileupload": "*",
		"express-pino-logger": "*",
		"express-session": "*",
		"express-validator": "*",
		"form-data": "*",
		"govuk-frontend": "*",
		"helmet": "*",
		"html-escaper": "*",
		"i18next": "*",
		"i18next-fs-backend": "*",
		"i18next-http-middleware": "*",
		"lodash": "*",
		"marked": "*",
		"moment": "*",
		"node-fetch": "*",
		"nunjucks": "*",
		"nunjucks-date-filter": "*",
		"ol": "*",
		"pino": "*",
		"proj4": "*",
		"redis": "*",
		"slugify": "*",
		"string-collapse-white-space": "*",
		"string-strip-html": "*",
		"uuid": "*"
	},
	"devDependencies": {
		"@babel/core": "*",
		"@babel/preset-env": "*",
		"babel-loader": "*",
		"eslint": "*",
		"eslint-config-prettier": "*",
		"eslint-plugin-jest": "*",
		"jest": "*",
		"jest-environment-jsdom": "*",
		"jest-fetch-mock": "*",
		"jest-junit": "*",
		"node-html-parser": "*",
		"nodemon": "*",
		"pino-pretty": "*",
		"sass": "*",
		"supertest": "*",
		"turbo": "*",
		"webpack": "*",
		"webpack-cli": "*",
		"webpack-merge": "*"
	}
}
