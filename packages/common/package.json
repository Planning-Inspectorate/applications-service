{
	"name": "@pins/common",
	"version": "0.0.0",
	"description": "Common modules to use between services",
	"private": true,
	"engines": {
		"node": ">=16.14.0"
	},
	"main": "src/index.js",
	"scripts": {
		"db:migrate:dev": "npx prisma migrate dev",
